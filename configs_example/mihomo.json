function getManualProxiesByRegex(params, regex) {
    const matchedProxies = params.proxies
        .filter((e) => regex.test(e.name))
        .map((e) => e.name);
    return matchedProxies.length > 0
        ? matchedProxies
        : [
        "DIRECT",
        "æ‰‹åŠ¨é€‰æ‹©", proxyName
    ];
}
